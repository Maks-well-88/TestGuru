<h2>Войдите:</h2>
<%= flash_message(:alert) %>

<%= form_with url: sessions_path do |f| %>
  <p>
    <%= f.label :email %>:
    <%= f.text_field :email, placeholder: 'введите email' %>
  </p>
  <p>
    <%= f.label :password %>:
    <%= f.text_field :password, placeholder: 'введите пароль' %>
  </p>

  <p>
    <%= f.submit 'Войти' %>
  </p>

<% end %>
<br />
